<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flyingpig.mapper.SupervisionMapper" >
    <select id="selectSupervisionIdByStudentId" resultType="integer">
        select id from supervision
        where student_id=#{studentId}
    </select>
</mapper>